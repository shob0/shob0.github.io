"generated":{"line":75,"column":0},"source":"js/Blogs/Post.js","original":{"line":65,"column":12}},{"generated":{"line":76,"column":0},"source":"js/Blogs/Post.js","original":{"line":46,"column":15}},{"generated":{"line":76,"column":1},"source":"js/Blogs/Post.js","original":{"line":46,"column":0}},{"name":"Post","generated":{"line":77,"column":15},"source":"js/Blogs/Post.js","original":{"line":72,"column":15}}],"sources":{"js/Blogs/Post.js":"import React from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\n/**\r\n * post object in props contains beloe value ie. props.post\r\n * id: Number\r\n * Title: String\r\n * Content: Long text String\r\n * Date Created: Date\r\n * Author: String\r\n *\r\n */\r\nfunction Post(props) {\r\n  let history = useHistory();\r\n\r\n  let post = props.post;\r\n  let contentShow = post.content.substring(0, 105);\r\n\r\n  return (\r\n    <div className=\"post sideDiv\" style={styles.post}>\r\n      <button\r\n        type=\"button\"\r\n        style={styles.btn}\r\n        className=\"post-btn\"\r\n        onClick={() => {\r\n          history.push(\"/blogs/\" + post.id);\r\n        }}\r\n      >\r\n        <div style={styles.postData}>\r\n          <div>\r\n            <hr className=\"divider\" />\r\n          </div>\r\n          <h2>{post.title}</h2>\r\n        </div>\r\n        <div className=\"post-content\" style={styles.postContent}>\r\n          <span style={styles.contentSpan}>\r\n            {contentShow}\r\n            {contentShow.length < post.content.length && <span>....</span>}\r\n          </span>\r\n        </div>\r\n      </button>\r\n      <hr />\r\n    </div>\r\n  );\r\n}\r\n\r\nconst styles = {\r\n  post: { cursor: \"pointer\", marginTop: \"1px\" },\r\n  postContent: {\r\n    marginLeft: \"30px\",\r\n    display: \"box\",\r\n  },\r\n  contentSpan: {\r\n    width: \"75vw\",\r\n    overflow: \"hidden\",\r\n    display: \"block\",\r\n    textOverflow: \"ellipsis\",\r\n  },\r\n  btn: {\r\n    border: \"none\",\r\n    textAlign: \"left\",\r\n    font: \"inherit\",\r\n    backgroundColor: \"inherit\",\r\n    cursor: \"pointer\",\r\n  },\r\n  postData: {\r\n    display: \"flex\",\r\n    alignItems: \"center\",\r\n    justifyContent: \"flex-start\",\r\n  },\r\n};\r\n\r\nexport default Post;\r\n"},"lineCount":null}},"error":null,"hash":"699e999f78914ce74b557db50ae65cf8","cacheData":{"env":{}}}